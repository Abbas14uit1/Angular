{
  "name": "sip",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:logging": "cross-env LOGGING=true npm run start",
    "build:fixtures": "cpy \"**/*.*\" \"../../../build/test/fixtures\" --parents --cwd=\"./server/ts/test/fixtures\"",
    "watch": "npm run build && concurrently -r --kill-others \"npm run --silent build:main -- -w\" \"sleepms 2000 && ava --watch\"",
    "webpack-dev-server": "webpack-dev-server -d --context ./client --config ./client/webpack.config.js --colors --progress",
    "client": "webpack-dev-server -d --context ./client --config ./client/webpack.config.js --colors --progress",
    "lint": "npm run lint:backend",
    "lint:backend": "tslint -c server/tslint/tslint-dev.json -p server/tsconfig.json server/ts/src/**/*.ts",
    "lint:frontend": "tslint -c client/tslint.json -p client/src/tsconfig.json client/src/**/*.ts",
    "unit": "npm run build:server && nyc ava",
    "test": "npm run lint && npm run unit && npm run check-coverage",
    "cov": "npm run unit && npm run html-coverage && opn coverage/index.html",
    "docs": "npm run docs:html && opn docs/server/index.html",
    "docs:html": "typedoc server/ts/src --mode modules --theme default --includeDeclarations --out docs/server --readme README.md --module commonjs --target ES6 --name \"Athlyte Football Portal\"",
    "changelog": "standard-version",
    "html-coverage": "nyc report --reporter=html",
    "gitlab-coverage": "nyc report --reporter=text-summary",
    "webpack:prod": "ng build --prod",
    "build:prod": "cross-env NODE_ENV=production npm run build:server",
    "webpack:qa": "ng build --prod --environment=qa",
    "build:qa": "cross-env NODE_ENV=qa npm run build:server",
    "webpack:demo": "ng build --prod --environment=demo",
    "build:demo": "cross-env NODE_ENV=demo npm run build:server",
    "ng": "ng",
    "frontend": "ng serve",
    "build": "ng build",
    "check-coverage": "nyc check-coverage --lines 75 --functions 70 --branches 75",
    "server": "npm run build:server && npm run start",
    "build:main": "tsc -p server/tsconfig.json",
    "build:server": "del-cli ./server/build && npm run build:server:main && npm run build:server:fixtures",
    "build:server:main": "tsc -p server/tsconfig.json",
    "build:server:fixtures": "cpy \"**/*.*\" \"../../../build/test/fixtures\" --parents --cwd=\"./server/ts/test/fixtures\"",
    "start": "node ./server/build/src/bin/www.js"
  },
  "private": true,
  "dependencies": {
